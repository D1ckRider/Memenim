<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Memenim.TabLayout"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="ResourceDictionaryName">TabLayout_Default</sys:String>


    <x:Array x:Type="sys:String" x:Key="TriggeredOnPage">
        <sys:String>FeedPage</sys:String>
        <sys:String>PlaceholderPage</sys:String>
        <sys:String>SettingsPage</sys:String>
    </x:Array>
    
    <x:Array Type="local:NavRedirectButtonData" x:Key="CentralNav">
        <local:NavRedirectButtonData PageName="PlaceholderPage" IconKind="Magnify"/>
        <local:NavRedirectButtonData PageName="FeedPage" IconKind="Newspaper"/>
        <local:NavRedirectButtonData PageName="Placeholder" IconKind="Chat"/>
    </x:Array>
</ResourceDictionary>